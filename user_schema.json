{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "birthdate": {
        "type": "string",
        "format": "date-time",
        "description": "The birthdate of the user (timestamp)"
      },
      "created_time": {
        "type": "string",
        "format": "date-time",
        "description": "The time when the user was created (timestamp)"
      },
      "display_name": {
        "type": "string",
        "description": "The display name of the user"
      },
      "email": {
        "type": "string",
        "format": "email",
        "description": "The email address of the user"
      },
      "photo_url": {
        "type": "string",
        "format": "uri",
        "description": "The URL to the user's profile photo"
      },
      "uid": {
        "type": "string",
        "description": "The unique identifier for the user"
      }
    },
    "required": ["birthdate", "created_time", "display_name", "email", "uid"]
  }
  